    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3",
    "@types/react": "16.7.22",
    "@types/react-dom": "16.0.11",
    "@types/react-redux": "latest",
    "office-ui-fabric-react": "6.148.1"
